{
  "name": "recruitment-brainhub-back",
  "version": "1.0.0",
  "description": "Recruitment node application",
  "main": "index.ts",
  "scripts": {
    "build": "rimraf ./build && tsc",
    "dev": "nodemon ./src/index.ts",
    "start": "npm run build && ts-node ./build/index.js",
    "dev:test": "jest --watch --coverage --verbose",
    "test:integration": "npm test -- --config=test/jest.config.ts --runInBand",
    "test:integration:api": "npm run test:integration -- --testPathPattern=./src/test/integration",
    "dev:test:unit": "npm test -- --coverage --testPathPattern=__test__"
  },
  "author": "Zygmunt Buszyk",
  "license": "ISC",
  "dependencies": {
    "celebrate": "^15.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "joi": "^17.8.3",
    "mysql2": "^3.2.0",
    "ts-node": "^10.9.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.2",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.1",
    "jest": "^29.4.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  }
}
